{
    "name" : "Extension",
    "manifest_version" : 3,
    "version" : "1.0",
    "description" : "A web extension on the chrome browser",
    "icons" : {
    "16": "images/16.png",
    "32": "images/32.png",
    "48": "images/48.png",
    "128": "images/128.png"
    },
    "background": {
        "service_worker": "background.js"
    },
    "action": {
        "default_icon": {
        "16": "images/16.png",
        "32": "images/32.png",
        "48": "images/48.png",
        "128": "images/128.png"
        }
    },
    "permissions": ["activeTab" , "scripting"]
}